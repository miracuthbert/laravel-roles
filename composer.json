{
    "name": "miracuthbert/laravel-roles",
    "description": "An Eloquent roles and permissions package for Laravel 5.8 and up",
    "keywords": ["laravel", "roles", "permissions"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cuthbert Mirambo",
            "email": "miracuthbert@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "kalnoy/nestedset": "^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0|^10.0|^11.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "laravel/legacy-factories": ">=1.0.4"
    },
    "autoload": {
        "psr-4": {
            "Miracuthbert\\LaravelRoles\\": "src/",
            "Miracuthbert\\LaravelRoles\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Miracuthbert\\LaravelRoles\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Miracuthbert\\LaravelRoles\\LaravelRolesServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "@php ./vendor/bin/phpunit"
        ]
    }
}
